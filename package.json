{
  "name": "rabbithole-cli",
  "version": "1.0.0",
  "description": "How deep does your dependency tree go? â€” Dependency health check CLI for vulnerabilities, outdated, deprecated, and stale packages.",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "bin": {
    "rabbithole": "dist/index.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/index.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "check": "npm run lint && npm run format:check && npm run build && npm run test",
    "prepublishOnly": "npm run check"
  },
  "keywords": [
    "dependencies",
    "audit",
    "outdated",
    "deprecated",
    "stale",
    "health",
    "cli",
    "security",
    "vulnerabilities",
    "npm"
  ],
  "author": "Miguel Medeiros",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MiguelMedeiros/rabbithole.git"
  },
  "homepage": "https://github.com/MiguelMedeiros/rabbithole#readme",
  "bugs": {
    "url": "https://github.com/MiguelMedeiros/rabbithole/issues"
  },
  "engines": {
    "node": ">=20"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/node": "^22.0.0",
    "@vitest/coverage-v8": "^4.0.18",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "prettier": "^3.8.1",
    "typescript": "^5.7.0",
    "typescript-eslint": "^8.55.0",
    "vitest": "^4.0.18"
  },
  "dependencies": {
    "@inquirer/prompts": "^7.0.0",
    "chalk": "^5.4.0",
    "cli-table3": "^0.6.5",
    "commander": "^13.0.0",
    "ora": "^8.2.0"
  }
}
